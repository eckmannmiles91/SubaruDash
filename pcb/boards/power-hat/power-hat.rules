
(rules PCB power-hat.dsn
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.7)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.4)
    )
  )
  (rule
    (width 200.0)
    (clearance 200.0)
    (clearance 100.0 (type smd_to_turn_gap))
    (clearance 50.0 (type smd))
    (clearance 200.0 (type "kicad_default"))
    (clearance 200.0 (type Power,Default))
  )
  (padstack "Via[0-1]_600:300_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:300_um" "Via[0-1]_600:300_um" default
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_600:300_um-kicad_default" "Via[0-1]_600:300_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:300_um-Power,Default" "Via[0-1]_600:300_um" Power,Default
  )
  (via 
    "Via[0-1]_800:400_um-Power,Default" "Via[0-1]_800:400_um" Power,Default
  )
  (via_rule
    default "Via[0-1]_600:300_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:300_um-kicad_default"
  )
  (via_rule
    Power,Default "Via[0-1]_800:400_um-Power,Default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    +3.3V "IGN_DETECT" XTAL1 XTAL2 "/U1_BOOT" "/COMP_RC" "12V_ACC" "unconnected-(J2-Pin_3-Pad3)"
    "FAN_TACH" "unconnected-(J2-Pin_37-Pad37)" "unconnected-(J2-Pin_29-Pad29)" "unconnected-(J2-Pin_10-Pad10)" "unconnected-(J2-Pin_8-Pad8)" "unconnected-(J2-Pin_22-Pad22)" "unconnected-(J2-Pin_40-Pad40)" "unconnected-(J2-Pin_23-Pad23)"
    "unconnected-(J2-Pin_36-Pad36)" "unconnected-(J2-Pin_38-Pad38)" "unconnected-(J2-Pin_18-Pad18)" "unconnected-(J2-Pin_16-Pad16)" "unconnected-(J2-Pin_7-Pad7)" "unconnected-(J2-Pin_28-Pad28)" "unconnected-(J2-Pin_5-Pad5)" "unconnected-(J2-Pin_13-Pad13)"
    "unconnected-(J2-Pin_26-Pad26)" "unconnected-(J2-Pin_12-Pad12)" "SHUTDOWN_REQ" "unconnected-(J2-Pin_11-Pad11)" "unconnected-(J2-Pin_27-Pad27)" "unconnected-(J2-Pin_21-Pad21)" "unconnected-(J2-Pin_24-Pad24)" "unconnected-(J2-Pin_35-Pad35)"
    "unconnected-(J2-Pin_19-Pad19)" "unconnected-(J2-Pin_31-Pad31)" "unconnected-(J3-Pin_2-Pad2)" "unconnected-(J3-Pin_1-Pad1)" "unconnected-(J3-Pin_6-Pad6)" "unconnected-(J3-Pin_5-Pad5)" "unconnected-(J3-Pin_4-Pad4)" "unconnected-(J3-Pin_3-Pad3)"
    FAN+ "FAN-" "unconnected-(JP1-Pin_1-Pad1)" "unconnected-(JP1-Pin_2-Pad2)" "unconnected-(JP2-Pin_1-Pad1)" "unconnected-(JP2-Pin_2-Pad2)" "unconnected-(JP2-Pin_3-Pad3)" "12V_SWITCHED"
    "/Q1_GATE" "GATE_CTRL" "FAN_PWM" "/U1_FB" "R6_OUT" RESET "Q2_GATE" "/U1_COMP"
    "/RT_CLK" "HEARTBEAT_LED" "TIMER_LED" "U1-FB" "12V_IGN"
    (clearance_class "kicad_default")
    (via_rule "kicad_default")
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Power,Default
    GND +5V "12V_FUSED" "/U1_SW"
    (clearance_class Power,Default)
    (via_rule Power,Default)
    (rule
      (width 600.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)